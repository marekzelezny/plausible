{
    "name": "marekzelezny/plausible",
    "description": "Simple package for managing Plausible with PHP",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marek Železný",
            "email": "marekzelezny@me.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.24|^9.0|^10.0",
        "illuminate/view": "^8.24|^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "orchestra/testbench": "^6.23|^7.0|^8.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "MarekZelezny\\Plausible\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarekZelezny\\Plausible\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarekZelezny\\Plausible\\PlausibleServiceProvider"
            ]
        }
    }
}
